<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate" />
  </configSections>
  <connectionStrings>
    <add name="TestConnectionString" connectionString="Server=localhost\sqlexpress;Database=BPELWorkflow;Integrated Security=SSPI" />

  </connectionStrings>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_BPELWorkFlowService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:9090/WorkFlowWCFService"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_BPELWorkFlowService"
                contract="WFService.BPELWorkFlowService" name="BasicHttpBinding_BPELWorkFlowService" />
        </client>
    </system.serviceModel>
  
  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2" >
    <session-factory>
      <property name="connection.driver_class">NHibernate.Driver.SqlClientDriver</property>
      <property name="connection.connection_string_name">TestConnectionString</property>
      <property name="adonet.batch_size">10</property>
      <property name="show_sql">true</property>
      <property name="dialect">NHibernate.Dialect.MsSql2008Dialect</property>
      <property name="command_timeout">10</property>
      <property name="query.substitutions">true 1, false 0, yes 'Y', no 'N'</property>
 
      <mapping assembly="NHibernate.DomainModel"/>
    </session-factory>
  </hibernate-configuration>

 
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
